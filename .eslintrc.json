{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "QUnit": true
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "esversion": 5
  },
  "rules": {
    "indent": [
      "error",
      2,
      { "SwitchCase": 1 }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}